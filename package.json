{
  "name": "s3-migration",
  "version": "1.0.0",
  "description": "AWS S3 bucket migration tool between AWS accounts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "vitest",
    "lint": "biome lint .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "dotenv": "^16.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@biomejs/biome": "1.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.3.1"
  }
} 